{
  "name": "@true-north-audio/backend-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@true-north-audio/backend"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "backend-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@true-north-audio/backend:build",
          "@true-north-audio/backend:serve"
        ]
      },
      "lint": {
        "cache": true,
        "options": {
          "cwd": "backend-e2e",
          "command": "eslint . --ignore-pattern dist --ignore-pattern build --ignore-pattern out --ignore-pattern test-output"
        }
      }
    }
  }
}
