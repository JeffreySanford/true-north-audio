{
  "name": "@true-north-audio/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
  "serve:fastapi": "set PYTHONPATH=.;ai-music-gen && python -m libs.musicgen.api",
  "serve:ollama": "C:/repos/true-north-audio/.venv/Scripts/python.exe -m uvicorn libs.musicgen.olamma_api:app --host 0.0.0.0 --port 11434",
  "validate:ollama": "curl --fail http://localhost:11434/olamma/status || (echo 'Ollama failed to start' && exit 1)",
     "serve:all": "pnpm lint:all && pnpm build:all && pnpm test:all && concurrently --kill-others-on-fail \"nx run backend:serve\" \"pnpm serve:ollama\" \"pnpm serve:fastapi\" \"nx run frontend:serve --proxy-config=src/proxy.conf.json\" && pnpm validate:ollama",
    "lint:backend": "nx run backend:lint",
    "lint:frontend": "nx run frontend:lint",
    "lint:backend-e2e": "nx run backend-e2e:lint",
    "lint:frontend-e2e": "nx run frontend-e2e:lint",
    "build:backend": "nx run backend:build",
    "build:frontend": "nx run frontend:build",
    "build:backend-e2e": "nx run backend-e2e:build",
    "build:frontend-e2e": "nx run frontend-e2e:build",
    "serve:backend": "nx run backend:serve",
    "serve:frontend": "nx run frontend:serve --proxy-config=src/proxy.conf.json",
    "serve:backend-e2e": "nx run backend-e2e:serve",
    "serve:frontend-e2e": "nx run frontend-e2e:serve",
    "test:backend": "nx run backend:test",
    "test:frontend": "nx run frontend:test",
    "test:backend-e2e": "nx run backend-e2e:test",
    "test:frontend-e2e": "nx run frontend-e2e:test",
  "lint:python": "python -m flake8 ai-music-gen/",
  "lint:python:fix": "python -m autopep8 --in-place --recursive ai-music-gen/",
  "test:python": "python -m pytest ai-music-gen/tests/",
  "build:python": "echo 'Python build step (optional)'",
  "lint:all": "nx run backend:lint && nx run frontend:lint && nx run backend-e2e:lint && nx run frontend-e2e:lint && pnpm lint:python",
  "sync:audio": "bash sync-generated-audio.sh",
  "lint:all:fix": "nx run backend:lint --fix && nx run frontend:lint --fix && nx run backend-e2e:lint --fix && nx run frontend-e2e:lint --fix && pnpm lint:python:fix && pnpm lint:python",
  "build:all": "nx run-many --target=build --all && pnpm build:python",
  "test:all": "nx run-many --target=test --all && pnpm test:python",
  "clean:all": "nx reset && rm -rf dist .nx node_modules/.cache coverage **/dist **/.next **/.turbo && bash clear-generated-melody.sh && echo 'Workspace cleaned.' && pnpm install",
  "clear:melody": "bash clear-generated-melody.sh"
  },
  "private": true,
